{
  "name": "lew-ui",
  "type": "module",
  "version": "2.7.55",
  "description": "A Component Library for Vue3.js.",
  "author": "lewkamtao",
  "license": "MIT",
  "keywords": [
    "vue3",
    "components",
    "ui library",
    "frontend"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./style": "./dist/index.css"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "package.json"
  ],
  "engines": {
    "node": "^22.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build:tsdown": "tsdown",
    "build:docs": "pnpm lint:fix && pnpm lint:css:fix && node ./extract-types.js && vue-tsc --noEmit && vite build --mode docs",
    "build:lib": "node ./bump-version.js && vue-tsc --noEmit && vite build --mode lib",
    "analyze": "vite build --mode analyze",
    "format": "pnpm lint:fix && pnpm lint:css:fix && node ./extract-types.js",
    "format:types": "node ./extract-types.js",
    "lint:css": "stylelint \"lib/**/*.{css,scss}\" \"docs/**/*.{css,scss}\"",
    "lint:css:fix": "stylelint \"lib/**/*.{css,scss}\" \"docs/**/*.{css,scss}\" --fix",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.5.3"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.12",
    "@vueuse/core": "^13.5.0",
    "axios": "^1.6.7",
    "csstype": "^3.1.3",
    "dayjs": "^1.11.13",
    "expr-eval": "^2.0.2",
    "lodash-es": "^4.17.21",
    "lucide-vue-next": "^0.534.0",
    "rollup-plugin-scss": "^4.0.1",
    "shiki-code-block-vue": "0.0.6-beta.1",
    "stylelint-config-recommended": "^16.0.0",
    "terser": "^5.44.0",
    "tippy.js": "^6.3.7",
    "ts-morph": "^26.0.0",
    "vue-i18n": "^11.1.0",
    "vue-router": "^4.0.13",
    "vue-virt-list": "^1.5.11",
    "vuedraggable": "^4.1.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.0.0",
    "@types/node": "^22.16.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-format": "^1.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.89.2",
    "stylelint": "16.2.1",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-order": "^7.0.0",
    "tsdown": "^0.14.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue": "^7.0.1",
    "vite": "^7.0.2",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-zip-pack": "^1.2.3",
    "vue": "^3.5.17",
    "vue-tsc": "^3.0.1"
  }
}
